#################################################################
#    General axioms
#################################################################

[ rdf:type owl:AllDisjointClasses ;
  owl:members ( :Business
                :CoupleLeisure
                :FamilyLeisure
                :SoloLeisure
              )
] .


[ rdf:type owl:AllDisjointClasses ;
  owl:members ( :BusinessClass
                :EconomyClass
                :FirstClass
                :PremiumEconomy
              )
] .


[ rdf:type owl:AllDisjointClasses ;
  owl:members ( :NotRecommended
                :RecomendationUnknown
                :Recommended
              )
] .


