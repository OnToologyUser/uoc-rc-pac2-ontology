@prefix : <http://www.semanticweb.org/alex/ontologies/2024/3/airline-rating/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.semanticweb.org/alex/ontologies/2024/3/airline-rating/> .

<http://www.semanticweb.org/alex/ontologies/2024/3/airline-rating> rdf:type owl:Ontology ;
                                                                    rdfs:comment "This ontology is related to flight satisfaction surveys. Its data is extracted from Kaggle and the companies Qatar Airways and Ryanair. This is a practice exercice for Knowladge Representation subject (Universitat Oberta de Catalunya)."@en .

#################################################################
#    Annotation properties
#################################################################

###  http://www.w3.org/2000/01/rdf-schema#sameAs
rdfs:sameAs rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/alex/ontologies/2024/3/airline-rating/answeredBy
:answeredBy rdf:type owl:ObjectProperty ;
            owl:inverseOf :hasAnswered ;
            rdfs:domain :Review ;
            rdfs:range :Passenger ;
            rdfs:comment "The review was answered by a passenger"@en ;
            rdfs:label "answeredBy"@en .


###  http://www.semanticweb.org/alex/ontologies/2024/3/airline-rating/flownWith
:flownWith rdf:type owl:ObjectProperty ;
           owl:inverseOf :hasPassenger ;
           rdfs:domain :Passenger ;
           rdfs:range :Flight ;
           rdfs:comment "The passenger flown with a flight"@en ;
           rdfs:label "flownWith"@en .


###  http://www.semanticweb.org/alex/ontologies/2024/3/airline-rating/hasAircraft
:hasAircraft rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :hasInformation ;
             rdfs:domain :Flight ;
             rdfs:range :Aircraft ;
             rdfs:comment "The flight has an associated aircraft, but we do not always know its aircraft."@en ;
             rdfs:label "hasAircraft"@en .


###  http://www.semanticweb.org/alex/ontologies/2024/3/airline-rating/hasAirline
:hasAirline rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf :hasInformation ;
            rdfs:domain :Flight ;
            rdfs:range :Airline ;
            rdfs:comment "The flight has an associated airline that operated it"@en ;
            rdfs:label "hasAirline"@en .


###  http://www.semanticweb.org/alex/ontologies/2024/3/airline-rating/hasAnswered
:hasAnswered rdf:type owl:ObjectProperty ;
             rdfs:domain :Passenger ;
             rdfs:range :Review ;
             rdfs:comment "The passenger answered a review"@en ;
             rdfs:label "hasAnswered"@en .


###  http://www.semanticweb.org/alex/ontologies/2024/3/airline-rating/hasDestination
:hasDestination rdf:type owl:ObjectProperty ;
                rdfs:domain :Flight ;
                rdfs:range :Location ;
                owl:propertyDisjointWith :hasOrigin ;
                rdfs:comment "The flight has as destination a location"@en ;
                rdfs:label "hasDestination"@en .


###  http://www.semanticweb.org/alex/ontologies/2024/3/airline-rating/hasInformation
:hasInformation rdf:type owl:ObjectProperty ;
                rdfs:domain :Flight ;
                rdfs:range :FlightInformation ;
                rdfs:comment "The flight has flight information"@en ;
                rdfs:label "hasInformation"@en .


###  http://www.semanticweb.org/alex/ontologies/2024/3/airline-rating/hasOrigin
:hasOrigin rdf:type owl:ObjectProperty ;
           rdfs:domain :Flight ;
           rdfs:range :Location ;
           rdfs:comment "The flight has as origin a location"@en ;
           rdfs:label "hasOrigin"@en .


###  http://www.semanticweb.org/alex/ontologies/2024/3/airline-rating/hasPassage
:hasPassage rdf:type owl:ObjectProperty ;
            rdfs:domain :Passenger ;
            rdfs:range :Passage ;
            rdfs:comment "The passenger has a passage"@en ;
            rdfs:label "hasPassage"@en .


###  http://www.semanticweb.org/alex/ontologies/2024/3/airline-rating/hasPassenger
:hasPassenger rdf:type owl:ObjectProperty ;
              rdfs:domain :Flight ;
              rdfs:range :Passenger ;
              rdfs:comment "The flight has passengers"@en ;
              rdfs:label "hasPassenger"@en .


###  http://www.semanticweb.org/alex/ontologies/2024/3/airline-rating/hasSeat
:hasSeat rdf:type owl:ObjectProperty ;
         rdfs:subPropertyOf :hasPassage ;
         rdfs:domain :Passenger ;
         rdfs:range :Seat ;
         rdfs:comment "The passenger has a seat"@en ;
         rdfs:label "hasSeat"@en .


###  http://www.semanticweb.org/alex/ontologies/2024/3/airline-rating/isFrom
:isFrom rdf:type owl:ObjectProperty ;
        rdfs:domain :Passenger ;
        rdfs:range :Country ;
        rdfs:comment "The passenger is from a country"@en ;
        rdfs:label "isFrom"@en .


###  http://www.semanticweb.org/alex/ontologies/2024/3/airline-rating/isPassengerType
:isPassengerType rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf :hasPassage ;
                 rdfs:domain :Passenger ;
                 rdfs:range :PassengerType ;
                 rdfs:comment "The passenger type info"@en ;
                 rdfs:label "isPassengerType"@en .


###  http://www.semanticweb.org/alex/ontologies/2024/3/airline-rating/isVerified
:isVerified rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf :hasPassage ;
            rdfs:domain :Passenger ;
            rdfs:range :Verification ;
            rdfs:comment "The passenger verification info"@en .


#################################################################
#    Data properties
#################################################################

###  http://www.semanticweb.org/alex/ontologies/2024/3/airline-rating/comment
:comment rdf:type owl:DatatypeProperty ;
         rdfs:domain :Review ;
         rdfs:range xsd:string ;
         rdfs:comment "The comment of the review made by the passenger"@en ;
         rdfs:label "comment"@en .


###  http://www.semanticweb.org/alex/ontologies/2024/3/airline-rating/connections
:connections rdf:type owl:DatatypeProperty ;
             rdfs:domain :Flight ;
             rdfs:range [ rdf:type rdfs:Datatype ;
                          owl:onDatatype xsd:unsignedShort ;
                          owl:withRestrictions ( [ xsd:minInclusive "1"^^xsd:unsignedShort
                                                 ]
                                               )
                        ] ;
             rdfs:comment "The number of connections of a flight. The minimum number of connections is 1"@en ;
             rdfs:label "connections"@en .


###  http://www.semanticweb.org/alex/ontologies/2024/3/airline-rating/dateFlown
:dateFlown rdf:type owl:DatatypeProperty ;
           rdfs:domain :Flight ;
           rdfs:range xsd:dateTime ;
           rdfs:comment "The date where the flight flown. As we only have the month and year, we will only set day as 1 and time 00:00:00"@en ;
           rdfs:label "dateFlown"@en .


###  http://www.semanticweb.org/alex/ontologies/2024/3/airline-rating/datePublished
:datePublished rdf:type owl:DatatypeProperty ;
               rdfs:domain :Review ;
               rdfs:range xsd:dateTime ;
               rdfs:comment "Date when the review was published by the passenger"@en ;
               rdfs:label "datePublished"@en .


###  http://www.semanticweb.org/alex/ontologies/2024/3/airline-rating/isRecommended
:isRecommended rdf:type owl:DatatypeProperty ;
               rdfs:subPropertyOf owl:topDataProperty ;
               rdfs:domain :Review ;
               rdfs:range [ rdf:type rdfs:Datatype ;
                            owl:onDatatype xsd:short ;
                            owl:withRestrictions ( [ xsd:minInclusive "-1"^^xsd:short
                                                   ]
                                                   [ xsd:maxInclusive "1"^^xsd:short
                                                   ]
                                                 )
                          ] ;
               rdfs:comment "If the passenger recommended the flight or not. There are cases where we do not know its value, so we have the following rules: 1 for recommended, 0 for not recommended and -1 for unkown"@en ;
               rdfs:label "isRecommended"@en .


###  http://www.semanticweb.org/alex/ontologies/2024/3/airline-rating/name
:name rdf:type owl:DatatypeProperty ;
      rdfs:subPropertyOf owl:topDataProperty ;
      rdfs:domain :Passenger ;
      rdfs:range xsd:string ;
      rdfs:comment "The passenger name"@en ;
      rdfs:label "name"@en .


###  http://www.semanticweb.org/alex/ontologies/2024/3/airline-rating/rating
:rating rdf:type owl:DatatypeProperty ;
        rdfs:domain :Review ;
        rdfs:range [ rdf:type rdfs:Datatype ;
                     owl:onDatatype xsd:unsignedShort ;
                     owl:withRestrictions ( [ xsd:minInclusive "1"^^xsd:unsignedShort
                                            ]
                                            [ xsd:maxInclusive "10"^^xsd:unsignedShort
                                            ]
                                          )
                   ] ;
        rdfs:comment "The review rating provided by the passenger, from 1 to 10"@en ;
        rdfs:label "rating"@en .


###  http://www.semanticweb.org/alex/ontologies/2024/3/airline-rating/title
:title rdf:type owl:DatatypeProperty ;
       rdfs:domain :Review ;
       rdfs:range xsd:string ;
       rdfs:comment "The title of the review made by the passenger"@en ;
       rdfs:label "title"@en .


#################################################################
#    Classes
#################################################################

###  http://www.semanticweb.org/alex/ontologies/2024/3/airline-rating/Aircraft
:Aircraft rdf:type owl:Class ;
          rdfs:subClassOf :FlightInformation ;
          rdfs:comment "The aircraft that operates one or more flights" ;
          rdfs:label "Aircraft"@en .


###  http://www.semanticweb.org/alex/ontologies/2024/3/airline-rating/Airline
:Airline rdf:type owl:Class ;
         rdfs:subClassOf :FlightInformation ;
         rdfs:comment "The airline that operates different flights"@en ;
         rdfs:label "Airline"@en .


###  http://www.semanticweb.org/alex/ontologies/2024/3/airline-rating/Business
:Business rdf:type owl:Class ;
          rdfs:subClassOf :PassengerType .


###  http://www.semanticweb.org/alex/ontologies/2024/3/airline-rating/BusinessClass
:BusinessClass rdf:type owl:Class ;
               rdfs:subClassOf :Seat .


###  http://www.semanticweb.org/alex/ontologies/2024/3/airline-rating/Country
:Country rdf:type owl:Class ;
         rdfs:comment "The country, currently this is related to passanger only, but in long term it could be related to other entites, like location, airline..."@en ;
         rdfs:label "Country"@en ;
         rdfs:sameAs "https://dbpedia.org/ontology/country"^^xsd:anyURI .


###  http://www.semanticweb.org/alex/ontologies/2024/3/airline-rating/CoupleLeisure
:CoupleLeisure rdf:type owl:Class ;
               rdfs:subClassOf :PassengerType .


###  http://www.semanticweb.org/alex/ontologies/2024/3/airline-rating/DirectFlight
:DirectFlight rdf:type owl:Class ;
              rdfs:subClassOf :Flight ;
              owl:disjointWith :TransitFlight ;
              rdfs:comment "The flight is direct if the number of connections is 1"@en ;
              rdfs:label "DirectFlight"@en .


###  http://www.semanticweb.org/alex/ontologies/2024/3/airline-rating/EconomyClass
:EconomyClass rdf:type owl:Class ;
              rdfs:subClassOf :Seat .


###  http://www.semanticweb.org/alex/ontologies/2024/3/airline-rating/FamilyLeisure
:FamilyLeisure rdf:type owl:Class ;
               rdfs:subClassOf :PassengerType .


###  http://www.semanticweb.org/alex/ontologies/2024/3/airline-rating/FirstClass
:FirstClass rdf:type owl:Class ;
            rdfs:subClassOf :Seat .


###  http://www.semanticweb.org/alex/ontologies/2024/3/airline-rating/Flight
:Flight rdf:type owl:Class ;
        rdfs:subClassOf [ rdf:type owl:Restriction ;
                          owl:onProperty :hasInformation ;
                          owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                          owl:onClass :FlightInformation
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :hasPassenger ;
                          owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                          owl:onClass :Passenger
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :hasAirline ;
                          owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                          owl:onClass :Airline
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :hasDestination ;
                          owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                          owl:onClass :Location
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :hasOrigin ;
                          owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                          owl:onClass :Location
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :hasAircraft ;
                          owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                          owl:onClass :Aircraft
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :connections ;
                          owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                          owl:onDataRange xsd:unsignedShort
                        ] ;
        rdfs:comment "The flight route offered by the airline on a specific date"@en ;
        rdfs:label "Flight"@en .


###  http://www.semanticweb.org/alex/ontologies/2024/3/airline-rating/FlightInformation
:FlightInformation rdf:type owl:Class ;
                   rdfs:comment "The flight detailed information, like the aircraft that operated the flight, the airline that operates the flight..."@en ;
                   rdfs:label "FlightInformation"@en .


###  http://www.semanticweb.org/alex/ontologies/2024/3/airline-rating/Location
:Location rdf:type owl:Class ;
          rdfs:comment "Location data. Currently, it's related to the origin and destination of a flight, but it can be related with other entites in the future, like the passenger data, the diferent stops of a transit flight..."@en ;
          rdfs:label "Location" ;
          rdfs:sameAs "https://dbpedia.org/ontology/Location"^^xsd:anyURI .


###  http://www.semanticweb.org/alex/ontologies/2024/3/airline-rating/NotRecommended
:NotRecommended rdf:type owl:Class ;
                rdfs:subClassOf :Review ;
                rdfs:comment "The result of the review is flight not recommended by the passenger. Equivalent to isRecommended equal 0"@en ;
                rdfs:label "NotRecommended"@en .


###  http://www.semanticweb.org/alex/ontologies/2024/3/airline-rating/NotSatisfiedPassenger
:NotSatisfiedPassenger rdf:type owl:Class ;
                       rdfs:subClassOf :Satisfaction ;
                       rdfs:comment "The passenger was not satisfied with the flight"@en ;
                       rdfs:label "NotSatisfiedPassenger"@en .


###  http://www.semanticweb.org/alex/ontologies/2024/3/airline-rating/NotVerified
:NotVerified rdf:type owl:Class ;
             rdfs:subClassOf :Verification ;
             owl:disjointWith :Verified .


###  http://www.semanticweb.org/alex/ontologies/2024/3/airline-rating/Passage
:Passage rdf:type owl:Class ;
         rdfs:comment "Passage info of the passenger"@en ;
         rdfs:label "Passage"@en .


###  http://www.semanticweb.org/alex/ontologies/2024/3/airline-rating/Passenger
:Passenger rdf:type owl:Class ;
           rdfs:subClassOf [ rdf:type owl:Restriction ;
                             owl:onProperty :hasPassage ;
                             owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                             owl:onClass :Passage
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :flownWith ;
                             owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                             owl:onClass :Flight
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :hasAnswered ;
                             owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                             owl:onClass :Review
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :hasSeat ;
                             owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                             owl:onClass :Seat
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :isFrom ;
                             owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                             owl:onClass :Country
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :isVerified ;
                             owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                             owl:onClass :Verification
                           ] ;
           rdfs:comment "The passenger data"@en ;
           rdfs:label "Passenger"@en ;
           rdfs:sameAs "http://dbpedia.org/ontology/Person"^^xsd:anyURI .


###  http://www.semanticweb.org/alex/ontologies/2024/3/airline-rating/PassengerType
:PassengerType rdf:type owl:Class ;
               rdfs:subClassOf :Passage ;
               rdfs:comment "The passenger type"@en ;
               rdfs:label "PassengerType"@en .


###  http://www.semanticweb.org/alex/ontologies/2024/3/airline-rating/PremiumEconomy
:PremiumEconomy rdf:type owl:Class ;
                rdfs:subClassOf :Seat .


###  http://www.semanticweb.org/alex/ontologies/2024/3/airline-rating/RecomendationUnknown
:RecomendationUnknown rdf:type owl:Class ;
                      rdfs:subClassOf :Review ;
                      rdfs:comment "We do not know if the passenger recommended or not the flight. Equivalent to isRecommended equal -1"@en .


###  http://www.semanticweb.org/alex/ontologies/2024/3/airline-rating/Recommended
:Recommended rdf:type owl:Class ;
             rdfs:subClassOf :Review ;
             rdfs:comment "The result of the review is flight recommended by the passenger. Equivalent to isRecommended equal 1"@en ;
             rdfs:label "Recommended"@en .


###  http://www.semanticweb.org/alex/ontologies/2024/3/airline-rating/Review
:Review rdf:type owl:Class ;
        rdfs:subClassOf [ rdf:type owl:Restriction ;
                          owl:onProperty :answeredBy ;
                          owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                          owl:onClass :Passenger
                        ] ;
        rdfs:comment "Review data of the survey, with info like rating, published date, comment..."@en ;
        rdfs:label "Review"@en .


###  http://www.semanticweb.org/alex/ontologies/2024/3/airline-rating/Satisfaction
:Satisfaction rdf:type owl:Class ;
              rdfs:subClassOf :Passenger ;
              rdfs:comment "Grade of satisfaction of the passenger, based on the review rating value. If the rating is higher or equal than 5, then the passenger is considered as satisifed, otherwise is considered as not satisfied"@en ;
              rdfs:label "Satisfaction"@en .


###  http://www.semanticweb.org/alex/ontologies/2024/3/airline-rating/SatisfiedPassenger
:SatisfiedPassenger rdf:type owl:Class ;
                    rdfs:subClassOf :Satisfaction ;
                    rdfs:comment "The passenger was satisfied with the flight"@en ;
                    rdfs:label "SatisfiedPassenger"@en .


###  http://www.semanticweb.org/alex/ontologies/2024/3/airline-rating/Seat
:Seat rdf:type owl:Class ;
      rdfs:subClassOf :Passage ;
      rdfs:comment "The seat info"@en ;
      rdfs:label "Seat"@en .


###  http://www.semanticweb.org/alex/ontologies/2024/3/airline-rating/SoloLeisure
:SoloLeisure rdf:type owl:Class ;
             rdfs:subClassOf :PassengerType .


###  http://www.semanticweb.org/alex/ontologies/2024/3/airline-rating/TransitFlight
:TransitFlight rdf:type owl:Class ;
               rdfs:subClassOf :Flight ;
               rdfs:comment "The flight is transit if the number of connections is greater than 1"@en ;
               rdfs:label "TransitFlight"@en .


###  http://www.semanticweb.org/alex/ontologies/2024/3/airline-rating/Verification
:Verification rdf:type owl:Class ;
              rdfs:subClassOf :Passage ;
              rdfs:comment "Verification represents if the passage was verified or not. If it was not verified, then we do not know if the passenger was a truthly passenger of the flight."@en ;
              rdfs:label "Verification" .


###  http://www.semanticweb.org/alex/ontologies/2024/3/airline-rating/Verified
:Verified rdf:type owl:Class ;
          rdfs:subClassOf :Verification .


#################################################################
#    Individuals
#################################################################

###  http://www.semanticweb.org/alex/ontologies/2024/3/airline-rating/A380-800
:A380-800 rdf:type owl:NamedIndividual ,
                   :Aircraft ;
          rdfs:comment "Aircraft model A380-800"@en ;
          rdfs:label "A380-800"@en .


###  http://www.semanticweb.org/alex/ontologies/2024/3/airline-rating/AlwaleedAlthaniQAT
:AlwaleedAlthaniQAT rdf:type owl:NamedIndividual ,
                             :Passenger ;
                    :flownWith :DohaLondon-Qatar-Feb2024 ;
                    :hasAnswered :review3qat ;
                    :hasSeat :FirstClass ;
                    :isFrom :Qatar ;
                    :isPassengerType :CoupleLeisure ;
                    :isVerified :Verified ;
                    :name "Alwaleed Althani" ;
                    rdfs:comment "The passenger Alwaleed Althani from Qatar"@en ;
                    rdfs:label "AlwaleedAlthaniQAT"@en .


###  http://www.semanticweb.org/alex/ontologies/2024/3/airline-rating/Boeing-737-900
:Boeing-737-900 rdf:type owl:NamedIndividual ,
                         :Aircraft ;
                rdfs:comment "Boing aircraft model 737-900"@en ;
                rdfs:label "Boeing-737-900"@en .


###  http://www.semanticweb.org/alex/ontologies/2024/3/airline-rating/Business
:Business rdf:type owl:NamedIndividual ,
                   :Business .


###  http://www.semanticweb.org/alex/ontologies/2024/3/airline-rating/BusinessClass
:BusinessClass rdf:type owl:NamedIndividual ,
                        :BusinessClass .


###  http://www.semanticweb.org/alex/ontologies/2024/3/airline-rating/CoupleLeisure
:CoupleLeisure rdf:type owl:NamedIndividual ,
                        :CoupleLeisure .


###  http://www.semanticweb.org/alex/ontologies/2024/3/airline-rating/Doha
:Doha rdf:type owl:NamedIndividual ,
               :Location ;
      rdfs:comment "Doha location"@en ;
      rdfs:label "Doha"@en .


###  http://www.semanticweb.org/alex/ontologies/2024/3/airline-rating/DohaLondon-Qatar-Feb2024
:DohaLondon-Qatar-Feb2024 rdf:type owl:NamedIndividual ,
                                   :Flight ;
                          :hasAircraft :A380-800 ;
                          :hasAirline :QatarAirways ;
                          :hasDestination :London ;
                          :hasOrigin :Doha ;
                          :connections "1"^^xsd:unsignedShort ;
                          :dateFlown "2024-02-01T00:00:00"^^xsd:dateTime ;
                          rdfs:comment "Flight from Doha to London, by Qatar Airways, flown at February 2024"@en ;
                          rdfs:label "DohaLondon-Qatar-Feb2024"@en .


###  http://www.semanticweb.org/alex/ontologies/2024/3/airline-rating/EconomyClass
:EconomyClass rdf:type owl:NamedIndividual ,
                       :EconomyClass .


###  http://www.semanticweb.org/alex/ontologies/2024/3/airline-rating/FamilyLeisure
:FamilyLeisure rdf:type owl:NamedIndividual ,
                        :FamilyLeisure .


###  http://www.semanticweb.org/alex/ontologies/2024/3/airline-rating/Faro
:Faro rdf:type owl:NamedIndividual ,
               :Location ;
      rdfs:comment "Faro location"@en ;
      rdfs:label "Faro"@en .


###  http://www.semanticweb.org/alex/ontologies/2024/3/airline-rating/FaroLuton-Ryanair-Feb2024
:FaroLuton-Ryanair-Feb2024 rdf:type owl:NamedIndividual ,
                                    :Flight ;
                           :hasAircraft :Boeing-737-900 ;
                           :hasDestination :Luton ;
                           :hasOrigin :Faro ;
                           :connections "1"^^xsd:unsignedShort ;
                           :dateFlown "2024-02-01T00:00:00"^^xsd:dateTime ;
                           rdfs:comment "Flight from Faro to Luton, by Ryanair, flown at February 2024"@en ;
                           rdfs:label "FaroLuton-Ryanair-Feb2024"@en .


###  http://www.semanticweb.org/alex/ontologies/2024/3/airline-rating/FirstClass
:FirstClass rdf:type owl:NamedIndividual ,
                     :FirstClass .


###  http://www.semanticweb.org/alex/ontologies/2024/3/airline-rating/London
:London rdf:type owl:NamedIndividual ,
                 :Location ;
        rdfs:comment "London location"@en ;
        rdfs:label "London"@en .


###  http://www.semanticweb.org/alex/ontologies/2024/3/airline-rating/Luton
:Luton rdf:type owl:NamedIndividual ,
                :Location ;
       rdfs:comment "Luton location"@en ;
       rdfs:label "Luton"@en .


###  http://www.semanticweb.org/alex/ontologies/2024/3/airline-rating/MaryLeUK
:MaryLeUK rdf:type owl:NamedIndividual ,
