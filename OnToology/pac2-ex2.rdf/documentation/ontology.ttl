@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://www.semanticweb.org/alex/ontologies/2024/3/airline-rating#> .

<http://www.semanticweb.org/alex/ontologies/2024/3/airline-rating> rdf:type owl:Ontology ;
                                                                    <http://purl.org/dc/elements/1.1/description> "This ontology is related to flight satisfaction surveys. Its data is extracted from Kaggle and the companies Qatar Airways and Ryanair. This is a practice exercice for Knowladge Representation subject (Universitat Oberta de Catalunya)."@en ;
                                                                    <http://purl.org/dc/elements/1.1/title> "Airline Rating"@en ;
                                                                    <http://purl.org/dc/terms/contributor> "https://github.com/alexgrauroca" ;
                                                                    <http://purl.org/dc/terms/created> "2024-04-17" ;
                                                                    <http://purl.org/dc/terms/creator> "https://github.com/alexgrauroca" ;
                                                                    <http://purl.org/dc/terms/license> "http://gnu.org/licenses/gpl-3.0.html" ;
                                                                    <http://schema.org/codeRepository> "https://github.com/alexgrauroca/uoc-rc-pac2-ontology" ;
                                                                    rdfs:label "Airline Rating"@en ;
                                                                    owl:versionInfo "0.0.1" .

